{
  "name": "flatlist-react",
  "version": "1.3.0",
  "description": "A helpful utility component to handle lists in react like a champ",
  "main": "./lib/index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "tsc": "tsc",
    "build": "rm -rf ./lib && npm run lint && tsc",
    "build:local": "npm run lint && npm run tsc",
    "local": "nodemon --watch ./src -e ts,tsx --exec npm run build:local",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ECorreia45/flatlist-react"
  },
  "keywords": [
    "react",
    "utility",
    "flatlist"
  ],
  "author": "Elson T. Correia",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.0"
  },
  "devDependencies": {
    "@types/core-js": "^2.5.2",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.6.9",
    "@types/react": "^16.8.24",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-json": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  }
}
