{
  "name": "flatlist-react",
  "version": "1.3.1",
  "description": "A helpful utility component to handle lists in react like a champ",
  "main": "./lib/index.js",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "rm -rf ./lib && npm run lint && tsc",
    "build:local": "npm run lint && npm run tsc",
    "local": "nodemon --watch ./src -e ts,tsx --exec npm run build:local",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ECorreia45/flatlist-react"
  },
  "keywords": [
    "react",
    "utility",
    "flatlist"
  ],
  "author": "Elson T. Correia",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@types/core-js": "^2.5.3",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.34",
    "@types/react": "^16.9.32",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "ts-jest": "^24.0.2",
    "tslint": "^5.20.1",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3"
  }
}
