{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [false],
    "indent": [true, "spaces", 4],
    "no-console": [false],
    "quotemark": [true, "single"],
    "object-literal-sort-keys": [false],
    "ordered-imports": [false],
    "no-namespace": [false],
    "interface-name": [true, "never-prefix"]
  },
  "rulesDirectory": []
}
